<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- تسلسل للوثائق الواردة -->
        <record id="sequence_incoming_document" model="ir.sequence">
            <field name="name">Incoming Document</field>
            <field name="code">archive.management.incoming</field>
            <field name="prefix">IN/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- تسلسل للوثائق الصادرة -->
        <record id="sequence_outgoing_document" model="ir.sequence">
            <field name="name">Outgoing Document</field>
            <field name="code">archive.management.outgoing</field>
            <field name="prefix">OUT/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- تسلسل للمذكرات الداخلية -->
        <record id="sequence_memo_document" model="ir.sequence">
            <field name="name">Memo Document</field>
            <field name="code">archive.management.memo</field>
            <field name="prefix">MEMO/%(year)s/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>